{
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true,
  },
  "plugins": [
    "react",
    "import",
  ],
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
  ],
  "parserOptions": {
    "sourceType": "module",
  },
}
